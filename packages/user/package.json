  "scripts": {
    "prisma:generate": "pnpm run prisma:dappstore:generate",
    "prisma:push": "pnpm run prisma:dappstore:push",
    "prisma:studio": "pnpm run prisma:dappstore:studio",
    "prisma:dappstore:generate": "prisma generate --schema src/prisma/schema.prisma",
    "prisma:dappstore:push": "dotenv -e ../../.env -c -- prisma db push --schema ./src/prisma/schema.prisma",
    "prisma:dappstore:studio": "dotenv -e ../../.env -c -- prisma studio --schema src/prisma/schema.prisma -p 5555",
    "postinstall": "pnpm run prisma:generate",
    "test": "dotenv -e ../../.env -c -- vitest ."
  },
