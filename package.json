{
  "name": "evmos-apps",
  "version": "2.0.4",
  "license": "SEE LICENSE IN LICENSE",
  "author": "Tharsis Labs Ltd.(Evmos)",
  "private": true,
  "scripts": {
    "build:production": "NEXT_PUBLIC_EVMOS_APP_ENV=production turbo run build --force --no-cache",
    "build": "dotenv -- turbo run build",
    "build:analyse": "ANALYZE_NEXT_BUNDLE=true next build",
    "nibble": "eslint-nibble . -c eslint.config.js",
    "dev": "dotenv -- turbo run dev --filter=./apps/*",
    "dev:testnet": "dotenv -- turbo run dev:testnet --concurrency 20",
    "start": "dotenv -- turbo run start",
    "testnet": "dotenv -- tsx node_modules/@evmosapps/testnet/src/cli/index.ts",
    "lint": "eslint .",
    "clean": "turbo run clean",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "playwright": "playwright test --headed --pass-with-no-tests",
    "test:e2e": "dotenv -- turbo run test:e2e --concurrency 1",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clear-cache": "sh ./clear-cache.sh",
    "kill": "turbo run kill",
    "prepare": "husky install",
    "clear-dev-cache": "rm -rf ./node_modules/.cache/evmosapps && echo 'Dev cache cleared!'",
    "license": "node ./check-license.mjs",
    "license:fix": "node ./check-license.mjs --fix"
  },
  "devDependencies": {
    "@evmosapps/config": "workspace:*",
    "@evmosapps/testnet": "workspace:*",
    "@types/react-dom": "^18.2.18",
    "eslint-plugin-vitest": "^0.3.20",
    "husky": "^8.0.0",
    "knip": "4.0.0-canary.8",
    "prettier": "^3.0.1",
    "react-dom": "^18.2.0",
    "viem": "^2.7.9",
    "wagmi": "^2.5.7"
  },
  "browserslist": [
    "Chrome 118",
    "last 3 chrome version",
    "last 3 firefox version",
    "last 3 safari version"
  ],
  "dependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-syntax-import-assertions": "^7.22.5",
    "@evmosapps/i18n": "workspace:*",
    "@headlessui/react": "^1.7.15",
    "@hookform/resolvers": "^3.1.0",
    "@playwright/test": "^1.40.1",
    "@tanstack/react-query": "^5.17.19",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/lodash-es": "^4.17.12",
    "@types/mixpanel-browser": "^2.48.1",
    "@types/react": "^18.2.47",
    "@vitest/coverage-v8": "^0.34.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.56.0",
    "eslint-nibble": "^8.1.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "globby": "^14.0.0",
    "helpers": "workspace:*",
    "jsdom": "^23.2.0",
    "kill-port": "^2.0.1",
    "playwright": "^1.40.1",
    "react-hook-form": "^7.44.3",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "turbo": "^1.10.16",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3",
    "zod": "^3.22.1"
  }
}
